<project name="jem-enterprise-properties" basedir=".">
	<!-- import common properties -->
	<import file="../JEM/build-common-properties.xml" />
	<!-- properties for JEM-enterprise project -->
	<property name="jem-junit.project.directory" value="../JEM-junit" />
	<property name="jem-junit.library.directory" value="${jem-junit.project.directory}/lib" />
