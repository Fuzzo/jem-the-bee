<?xml version="1.0"?>
<project name="TEST_FTP_RESOURCE_REMOVE" default="remove">
	<description>
		This job removes the FTP resource used to test FTP functionalities.
    </description>

	<taskdef name="resources" classname="org.pepstock.jem.ant.tasks.utilities.CommonResourcesTask" />

	<target name="remove">
		<resources>
			<dataDescription name="COMMAND" disposition="SHR">
				<dataSet>
					REMOVE JUNIT-FTP-RESOURCE;
				</dataSet>
			</dataDescription>
		</resources>
	</target>

</project>