<project name="jem-jbpm-properties" basedir=".">
	<!-- 
	Creates all properties used inside the build for JEM JBPM core project
	Many of these properties are based on the common ones 
	-->
	<!-- import common properties -->
	<import file="../JEM/build-common-properties.xml" />
	
	<!--properties for JEM JBPM project -->
	<property name="jem.jbpm.resources.folder" value="${jem.jbpm.folder}/resources" />
	<property name="jem.jbpm.jar.file" value="jem-jbpm.jar" />
	<property name="jem.jbpm.jar.file.location" location="${jem.jbpm.library.folder}/${jem.jbpm.jar.file}" />
	<property name="jem.jbpm.distribute.folder" value="${jem.jbpm.folder}/dist" />
	<property name="jem.jbpm.zip.file" value="jem-jbpm.zip" />
	<property name="jem.jbpm.zip.file.location" value="${jem.jbpm.distribute.folder}/${jem.jbpm.zip.file}" />
	<property name="jem.jbpm.workitem.distribute.folder" value="resources" />
</project>